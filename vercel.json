{
  "buildCommand": "cd frontend/vue-frontend && npm run build",
  "outputDirectory": "frontend/vue-frontend/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "backend/main.py": {
      "runtime": "python3.9"
    }
  }
}
